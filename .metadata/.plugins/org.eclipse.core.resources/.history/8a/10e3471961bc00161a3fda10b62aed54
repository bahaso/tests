package bahaso.testing.webElement;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class LitteBoxMultipleChoice extends ExcercisePage{
	public WebElement boxAnswer = null;
	public LitteBoxMultipleChoice(WebDriver driver){
		this.driver = driver;
	}
	
	public WebElement getBoxAnswer(int i,int j) {
		boxAnswer = wt.waitForElement(driver, By.xpath(".//input[@class='lesson']"));
		return boxAnswer;
	}
	
}
